{
  "name": "@balena/jellyfish-plugin-github",
  "description": "GitHub Jellyfish Plugin",
  "version": "5.1.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-github"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && deplint",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest test/unit",
    "test:integration": "jest --bail --runInBand test/integration",
    "test:compose": "docker build -t balena/jellyfish-sut:latest . && docker-compose -f docker-compose.test.yml -f docker-compose.yml up --exit-code-from=sut",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "compose": "docker-compose up"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.2.29",
    "@balena/jellyfish-worker": "^27.2.1",
    "@octokit/auth-app": "^3.6.1",
    "@octokit/plugin-retry": "^3.0.9",
    "@octokit/plugin-throttling": "^3.6.2",
    "@octokit/rest": "^18.12.0",
    "autumndb": "^20.1.0",
    "lodash": "^4.17.21",
    "uuid": "^8.3.2",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@balena/jellyfish-config": "^2.0.6",
    "@balena/jellyfish-environment": "^12.0.3",
    "@balena/jellyfish-plugin-default": "^27.3.1",
    "@balena/jellyfish-plugin-product-os": "^7.0.8",
    "@balena/jellyfish-types": "^2.0.5",
    "@balena/lint": "^6.2.0",
    "@types/jest": "^27.5.1",
    "@types/uuid": "^8.3.4",
    "date-fns": "^2.28.0",
    "deplint": "^1.1.3",
    "jest": "^28.1.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^12.4.2",
    "nock": "^13.2.4",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.7.0",
    "ts-jest": "^28.0.3",
    "typedoc": "^0.22.16",
    "typescript": "4.6.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2022-06-01T03:32:01.221Z"
  }
}
