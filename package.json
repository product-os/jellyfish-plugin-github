{
  "name": "@balena/jellyfish-plugin-github",
  "description": "GitHub Jellyfish Plugin",
  "version": "1.11.258",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-github"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && deplint",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest test/unit",
    "test:integration": "jest --runInBand --bail test/integration",
    "test:compose": "docker build -t balena/jellyfish-sut:latest . && docker-compose -f docker-compose.test.yml -f docker-compose.yml up --exit-code-from=sut",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "compose": "docker-compose up"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@balena/jellyfish-action-library": "^15.1.202",
    "@balena/jellyfish-assert": "^1.2.2",
    "@balena/jellyfish-plugin-base": "^2.2.5",
    "@octokit/auth-app": "^3.6.1",
    "@octokit/plugin-retry": "^3.0.9",
    "@octokit/plugin-throttling": "^3.5.2",
    "@octokit/rest": "^18.12.0",
    "lodash": "^4.17.21",
    "uuid": "^8.3.2",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@balena/jellyfish-config": "^2.0.2",
    "@balena/jellyfish-core": "^8.2.2",
    "@balena/jellyfish-environment": "^6.0.1",
    "@balena/jellyfish-plugin-default": "^21.1.147",
    "@balena/jellyfish-plugin-product-os": "^2.9.4",
    "@balena/jellyfish-test-harness": "^8.1.204",
    "@balena/jellyfish-types": "^1.2.39",
    "@balena/lint": "^6.2.0",
    "@types/jest": "^27.0.3",
    "@types/uuid": "^8.3.3",
    "date-fns": "^2.27.0",
    "deplint": "^1.1.3",
    "jest": "^27.4.5",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^12.1.3",
    "nock": "^13.2.1",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.7.0",
    "ts-jest": "^27.1.2",
    "typedoc": "^0.22.10",
    "typescript": "4.5.4"
  },
  "peerDependencies": {
    "@balena/jellyfish-core": "^8.2.2",
    "@balena/jellyfish-environment": "^6.0.1",
    "@balena/jellyfish-plugin-default": "^21.1.147"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-12-18T11:38:54.653Z"
  }
}
