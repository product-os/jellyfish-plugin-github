{
  "name": "@balena/jellyfish-plugin-github",
  "description": "GitHub Jellyfish Plugin",
  "version": "1.11.55",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-github"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && npm run check && deplint && depcheck --ignore-bin-package --ignores=@types/jest",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest test/unit",
    "test:integration": "jest --runInBand --bail test/integration",
    "test:compose": "DOCKER_BUILDKIT=1 docker build -t balena/jellyfish-plugin-github_sut:latest --secret id=npmrc,src=.balena/secrets/npmrc.txt . && docker-compose -f docker-compose.test.yml -f docker-compose.yml up --exit-code-from=sut",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck",
    "compose": "docker-compose up"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-action-library": "^15.1.48",
    "@balena/jellyfish-assert": "^1.1.67",
    "@balena/jellyfish-plugin-base": "^2.1.235",
    "@octokit/auth-app": "^3.6.0",
    "@octokit/plugin-retry": "^3.0.9",
    "@octokit/plugin-throttling": "^3.5.1",
    "@octokit/rest": "^18.9.1",
    "bluebird": "^3.7.2",
    "lodash": "^4.17.21",
    "uuid": "^8.3.2",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.3",
    "@balena/jellyfish-config": "^1.4.3",
    "@balena/jellyfish-core": "^5.7.1",
    "@balena/jellyfish-environment": "^5.0.1",
    "@balena/jellyfish-plugin-default": "^20.1.0",
    "@balena/jellyfish-test-harness": "^6.2.0",
    "@balena/jellyfish-types": "^1.1.0",
    "@balena/lint": "^6.1.1",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^27.0.1",
    "@types/uuid": "^8.3.1",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "jest": "^27.0.6",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^11.1.2",
    "nock": "^13.1.2",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.6.1",
    "ts-jest": "^27.0.5",
    "typedoc": "^0.21.6",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "@balena/jellyfish-core": "^5.7.1",
    "@balena/jellyfish-environment": "^5.0.1",
    "@balena/jellyfish-plugin-default": "^20.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-09-07T15:04:29.303Z"
  }
}
