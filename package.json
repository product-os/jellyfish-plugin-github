{
  "name": "@balena/jellyfish-plugin-github",
  "description": "GitHub Jellyfish Plugin",
  "version": "8.0.48",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-github"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "lib/contracts/*.ts": [
      "npm run types",
      "git add lib/types/contracts/*"
    ],
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && deplint",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest test/unit",
    "test:integration": "jest --bail --runInBand test/integration",
    "test:compose": "docker build -t balena/jellyfish-sut:latest . && docker-compose -f docker-compose.test.yml -f docker-compose.yml up --exit-code-from=sut",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "compose": "docker-compose up",
    "types": "npm run build && autumndb generate-contract-interfaces && balena-lint --fix lib/types/contracts"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.2.54",
    "@balena/jellyfish-environment": "^13.0.4",
    "@balena/jellyfish-worker": "^33.0.36",
    "@octokit/auth-app": "^4.0.5",
    "@octokit/plugin-retry": "^3.0.9",
    "@octokit/plugin-throttling": "^4.2.0",
    "@octokit/rest": "^19.0.4",
    "autumndb": "^22.1.28",
    "lodash": "^4.17.21",
    "uuid": "^8.3.2",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@balena/lint": "^6.2.0",
    "@types/jest": "^28.1.7",
    "@types/uuid": "^8.3.4",
    "date-fns": "^2.29.2",
    "deplint": "^1.1.3",
    "jest": "^28.1.3",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^13.0.3",
    "nock": "^13.2.9",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.0",
    "ts-jest": "^28.0.8",
    "typedoc": "^0.22.17",
    "typescript": "4.7.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2022-08-24T15:14:51.240Z"
  }
}
